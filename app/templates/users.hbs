<div class="row">
    {{#each model as |user|}}
    <div class="col-md-3">
        <div class="container-fluid well span6">
            <div class="row-fluid">
                <div class="span2">
                    <img src="" class="img-circle">
                </div>

                <div class="span8">
                    <h3>{{user.name}}</h3>
                    <h6>{{user.username}}</h6>
                    <h6>{{user.email}}</h6>
                    <address>
                        {{user.address.street}}
                        <br> {{user.address.suite}}
                        <br> {{user.address.city}}
                        <br> {{user.address.zipcode}}
                    
                        <br> {{user.address.geo.lat}} {{user.address.geo.lng}}
                    </address>
                </div>
            </div>
        </div>
    </div>
    {{/each}}
</div>
{{outlet}}
